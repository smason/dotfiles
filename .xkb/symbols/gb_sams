// see: /usr/share/X11/xkb
// lots of symbols are defined in:
//   /usr/share/X11/xkb/symbols/latin

default partial alphanumeric_keys
xkb_symbols "basic" {
  include "gb"
  name[Group1] = "English (UK, hacked)";
  key <AE11>  { [     minus,  underscore,     endash,      dead_macron ]      };
  key <AE12>  { [     equal,        plus,     emdash,      dead_ogonek ]      };
};
