Host *
  ControlPath ~/.ssh/controlmasters/%r@%h:%p
  ControlPersist 10m

Host staging staging.consultantconnect.org.uk
  User sam
  HostName staging.consultantconnect.org.uk
  ForwardAgent Yes

Host live www.consultantconnect.org.uk consultantconnect.org.uk
  User sam
  HostName www.consultantconnect.org.uk
  ForwardAgent Yes

Host github.com
  User git

Host stick jumpingstick
  HostName jumpingstick.co.uk

Host keepawake
  HostName jumpingstick.co.uk
  User keepawake
  IdentityFile ~/.ssh/id_keepawake

Host osx
  HostName localhost
  Port 2022
  ForwardAgent Yes
  RemoteForward /Users/smason/.gnupg/S.gpg-agent /run/user/1000/gnupg/S.gpg-agent.extra
  ControlMaster auto
